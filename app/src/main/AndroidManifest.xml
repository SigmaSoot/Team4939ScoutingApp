<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.magne.a2019roboticsapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DisplayMessageActivity" />
        <activity
            android:name=".ScrollingActivity"
            android:label="@string/title_activity_scrolling"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Competitions"
            android:label="@string/title_activity_competitions"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.magne.a2019roboticsapp.MainActivity" />
        </activity>
        <activity android:name=".RyersonSelection" />
        <activity android:name=".HumberSelection" />
        <activity
            android:name=".TeamScoring"
            android:label="@string/title_activity_team_scoring"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".RealTeamScoring"
            android:label="@string/title_activity_real_team_scoring"
            android:theme="@style/AppTheme.NoActionBar"
            android:parentActivityName=".RealTeamEndgame"/>
        <activity
            android:name=".RealTeamEndgame"
            android:label="@string/title_activity_real_team_endgame"
            android:theme="@style/AppTheme.NoActionBar"
            android:parentActivityName=".RealTeleOp"/>
        <activity
            android:name=".RealTeleOp"
            android:label="@string/title_activity_real_tele_op"
            android:theme="@style/AppTheme.NoActionBar"
            android:parentActivityName=".RealSandstorm"/>
        <activity
            android:name=".RealSandstorm"
            android:label="@string/title_activity_real_sandstorm"
            android:theme="@style/AppTheme.NoActionBar"
            android:parentActivityName=".RealBegginingLoc"/>
        <activity
            android:name=".RealBegginingLoc"
            android:label="@string/title_activity_real_beggining_loc"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".RealSubmit"
            android:label="@string/title_activity_real_submit"
            android:theme="@style/AppTheme.NoActionBar"
            android:parentActivityName=".RealTeamScoring"></activity>
    </application>

</manifest>
<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".RealTeleOp"
    tools:showIn="@layout/activity_real_tele_op">

    <CheckBox
        android:id="@+id/yesCargoTeleCB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="40dp"
        android:scaleX="0.90"
        android:scaleY="0.90"
        android:text="Yes"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/yesHatchCB"
        app:layout_constraintStart_toStartOf="@+id/yesHatchCB" />

    <CheckBox
        android:id="@+id/noCargoTeleCB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginTop="11dp"
        android:scaleX="0.90"
        android:scaleY="0.90"
        android:text="No"
        app:layout_constraintStart_toEndOf="@+id/yesCargoTeleCB"
        app:layout_constraintTop_toBottomOf="@+id/textView18" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="3dp"
        android:layout_marginBottom="11dp"
        android:text="Successful"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/yesCargoTeleCB"
        app:layout_constraintEnd_toStartOf="@+id/noCargoTeleCB" />

    <TextView
        android:id="@+id/cycleTitle"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginStart="35dp"
        android:layout_marginTop="6dp"
        android:layout_marginBottom="7dp"
        android:text="Cycle 1"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/hatchTeleCB"
        app:layout_constraintStart_toEndOf="@+id/hatchTeleCB"
        app:layout_constraintTop_toTopOf="parent" />

    <CheckBox
        android:id="@+id/hatchTeleCB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="43dp"
        android:layout_marginTop="47dp"
        android:scaleX="0.90"
        android:scaleY="0.90"
        android:text="Hatch"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <CheckBox
        android:id="@+id/rocketHatchTeleCB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="37dp"
        android:layout_marginTop="11dp"
        android:scaleX="0.90"
        android:scaleY="0.90"
        android:text="Rocketship"
        app:layout_constraintStart_toStartOf="@+id/hatchTeleCB"
        app:layout_constraintTop_toBottomOf="@+id/hatchTeleCB" />

    <CheckBox
        android:id="@+id/oneRocketHatchCB"
        android:layout_width="wrap_content"
        android:layout_height="27dp"
        android:layout_marginStart="41dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        android:scaleX="0.80"
        android:scaleY="0.80"
        android:text="Level 1"
        app:layout_constraintBottom_toTopOf="@+id/twoRocketHatchCB"
        app:layout_constraintStart_toStartOf="@+id/rocketHatchTeleCB"
        app:layout_constraintTop_toBottomOf="@+id/rocketHatchTeleCB" />

    <CheckBox
        android:id="@+id/twoRocketHatchCB"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_marginTop="79dp"
        android:layout_marginBottom="80dp"
        android:scaleX="0.80"
        android:scaleY="0.80"
        android:text="Level 2"
        app:layout_constraintBottom_toTopOf="@+id/textView17"
        app:layout_constraintStart_toStartOf="@+id/oneRocketHatchCB"
        app:layout_constraintTop_toTopOf="@+id/rocketHatchTeleCB" />

    <CheckBox
        android:id="@+id/threeRocketHatchCB"
        android:layout_width="wrap_content"
        android:layout_height="24dp"
        android:layout_marginTop="42dp"
        android:layout_marginBottom="41dp"
        android:scaleX="0.80"
        android:scaleY="0.80"
        android:text="Level 3"
        app:layout_constraintBottom_toBottomOf="@+id/cargoHatchCB"
        app:layout_constraintStart_toStartOf="@+id/twoRocketHatchCB"
        app:layout_constraintTop_toBottomOf="@+id/oneRocketHatchCB" />

    <CheckBox
        android:id="@+id/cargoHatchCB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="234dp"
        android:layout_marginEnd="26dp"
        android:scaleX="0.90"
        android:scaleY="0.90"
        android:text="Cargoship"
        app:layout_constraintEnd_toEndOf="@+id/textView17"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Successful"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/yesHatchCB"
        app:layout_constraintTop_toBottomOf="@+id/cargoHatchCB" />

    <CheckBox
        android:id="@+id/yesHatchCB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:scaleX="0.90"
        android:scaleY="0.90"
        android:text="Yes"
        app:layout_constraintEnd_toEndOf="@+id/cargoTeleCB"
        app:layout_constraintStart_toEndOf="@+id/cargoTeleCB"
        app:layout_constraintTop_toBottomOf="@+id/textView17" />

    <CheckBox
        android:id="@+id/noHatchCB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="1dp"
        android:layout_marginTop="8dp"
        android:scaleX="0.90"
        android:scaleY="0.90"
        android:text="No"
        app:layout_constraintStart_toEndOf="@+id/textView17"
        app:layout_constraintTop_toBottomOf="@+id/textView17" />

    <CheckBox
        android:id="@+id/cargoTeleCB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="51dp"
        android:layout_marginTop="10dp"
        android:scaleX="0.90"
        android:scaleY="0.90"
        android:text="Cargo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/yesHatchCB" />

    <CheckBox
        android:id="@+id/rocketCargoTeleCB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="47dp"
        android:layout_marginTop="7dp"
        android:scaleX="0.90"
        android:scaleY="0.90"
        android:text="Rocketship"
        app:layout_constraintStart_toStartOf="@+id/cargoTeleCB"
        app:layout_constraintTop_toBottomOf="@+id/cargoTeleCB" />

    <CheckBox
        android:id="@+id/oneRocketCargoTeleCB"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_marginTop="11dp"
        android:layout_marginEnd="3dp"
        android:layout_marginBottom="11dp"
        android:scaleX="0.80"
        android:scaleY="0.80"
        android:text="Level 1"
        app:layout_constraintBottom_toTopOf="@+id/twoRocketCargoCB"
        app:layout_constraintEnd_toEndOf="@+id/rocketCargoTeleCB"
        app:layout_constraintTop_toBottomOf="@+id/rocketCargoTeleCB" />

    <CheckBox
        android:id="@+id/twoRocketCargoCB"
        android:layout_width="wrap_content"
        android:layout_height="24dp"
        android:layout_marginTop="47dp"
        android:layout_marginBottom="45dp"
        android:scaleX="0.80"
        android:scaleY="0.80"
        android:text="Level 2"
        app:layout_constraintBottom_toTopOf="@+id/cargoCargoTeleCB"
        app:layout_constraintStart_toStartOf="@+id/oneRocketCargoTeleCB"
        app:layout_constraintTop_toBottomOf="@+id/rocketCargoTeleCB" />

    <CheckBox
        android:id="@+id/threeRocketCargoCB"
        android:layout_width="wrap_content"
        android:layout_height="24dp"
        android:layout_marginTop="118dp"
        android:layout_marginBottom="116dp"
        android:scaleX="0.80"
        android:scaleY="0.80"
        android:text="Level 3"
        app:layout_constraintBottom_toBottomOf="@+id/yesCargoTeleCB"
        app:layout_constraintStart_toStartOf="@+id/twoRocketCargoCB"
        app:layout_constraintTop_toBottomOf="@+id/cargoTeleCB" />

    <CheckBox
        android:id="@+id/cargoCargoTeleCB"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_marginTop="13dp"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="14dp"
        android:scaleX="0.90"
        android:scaleY="0.90"
        android:text="Cargoship"
        app:layout_constraintBottom_toTopOf="@+id/textView18"
        app:layout_constraintEnd_toEndOf="@+id/textView18"
        app:layout_constraintTop_toBottomOf="@+id/threeRocketCargoCB" />

    <Button
        android:id="@+id/button3"
        style="@android:style/Widget.Holo.Button"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="6dp"
        android:layout_marginEnd="6dp"
        android:layout_marginBottom="8dp"
        android:onClick="startStopChronometer"
        android:text="START"
        app:layout_constraintBottom_toTopOf="@+id/button5"
        app:layout_constraintEnd_toEndOf="@+id/button5"
        app:layout_constraintStart_toStartOf="@+id/button5" />

    <Button
        android:id="@+id/button5"
        style="@android:style/Widget.Holo.Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:onClick="nextCycle"
        android:text="END CYCLE"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/noHatchCB" />

    <Button
        android:id="@+id/button8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="22dp"
        android:layout_marginBottom="16dp"
        android:onClick="startEndgameActivity"
        android:text="NEXT"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <Chronometer
        android:id="@+id/chronometer1"
        android:layout_width="0dp"
        android:layout_height="32dp"

        android:layout_marginStart="16dp"
        android:layout_marginTop="69dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="70dp"
        android:countDown="false"
        android:textSize="22sp"
        app:layout_constraintBottom_toTopOf="@+id/button5"
        app:layout_constraintEnd_toEndOf="@+id/button3"
        app:layout_constraintStart_toStartOf="@+id/button5"
        app:layout_constraintTop_toTopOf="@+id/oneRocketHatchCB" />

</android.support.constraint.ConstraintLayout>
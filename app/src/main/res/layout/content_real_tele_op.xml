<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".RealTeleOp"
    tools:showIn="@layout/activity_real_tele_op">

    <CheckBox
        android:id="@+id/yesCargoTeleCB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="7dp"
        android:layout_marginRight="7dp"
        android:scaleX="0.90"
        android:scaleY="0.90"
        android:text="Yes"
        app:layout_constraintEnd_toEndOf="@+id/textView18"
        app:layout_constraintTop_toBottomOf="@+id/textView18" />

    <CheckBox
        android:id="@+id/noCargoTeleCB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="18dp"
        android:layout_marginLeft="18dp"
        android:scaleX="0.90"
        android:scaleY="0.90"
        android:text="No"
        app:layout_constraintBaseline_toBaselineOf="@+id/yesCargoTeleCB"
        app:layout_constraintStart_toEndOf="@+id/textView18" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:text="Successful"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/cargoCargoTeleCB"
        app:layout_constraintTop_toBottomOf="@+id/threeRocketCargoCB" />

    <TextView
        android:id="@+id/cycleTitle"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="200dp"
        android:layout_marginRight="200dp"
        android:layout_marginBottom="8dp"
        android:text="Cycle 1"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/hatchTeleCB"
        app:layout_constraintEnd_toStartOf="@+id/chronometer1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <CheckBox
        android:id="@+id/hatchTeleCB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="19dp"
        android:layout_marginLeft="19dp"
        android:layout_marginTop="58dp"
        android:scaleX="0.90"
        android:scaleY="0.90"
        android:text="Hatch"
        app:layout_constraintStart_toStartOf="@+id/cycleTitle"
        app:layout_constraintTop_toTopOf="parent" />

    <CheckBox
        android:id="@+id/rocketHatchTeleCB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="45dp"
        android:layout_marginLeft="45dp"
        android:scaleX="0.90"
        android:scaleY="0.90"
        android:text="Rocketship"
        app:layout_constraintStart_toStartOf="@+id/hatchTeleCB"
        app:layout_constraintTop_toBottomOf="@+id/hatchTeleCB" />

    <CheckBox
        android:id="@+id/oneRocketHatchCB"
        android:layout_width="wrap_content"
        android:layout_height="27dp"
        android:layout_marginStart="41dp"
        android:layout_marginLeft="41dp"
        android:layout_marginTop="29dp"
        android:layout_marginBottom="28dp"
        android:scaleX="0.80"
        android:scaleY="0.80"
        android:text="Level 1"
        app:layout_constraintBottom_toBottomOf="@+id/twoRocketHatchCB"
        app:layout_constraintStart_toStartOf="@+id/rocketHatchTeleCB"
        app:layout_constraintTop_toTopOf="@+id/rocketHatchTeleCB" />

    <CheckBox
        android:id="@+id/twoRocketHatchCB"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_marginTop="59dp"
        android:layout_marginBottom="60dp"
        android:scaleX="0.80"
        android:scaleY="0.80"
        android:text="Level 2"
        app:layout_constraintBottom_toBottomOf="@+id/cargoHatchCB"
        app:layout_constraintStart_toStartOf="@+id/oneRocketHatchCB"
        app:layout_constraintTop_toTopOf="@+id/rocketHatchTeleCB" />

    <CheckBox
        android:id="@+id/threeRocketHatchCB"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginBottom="4dp"
        android:scaleX="0.80"
        android:scaleY="0.80"
        android:text="Level 3"
        app:layout_constraintBottom_toTopOf="@+id/cargoHatchCB"
        app:layout_constraintStart_toStartOf="@+id/twoRocketHatchCB"
        app:layout_constraintTop_toBottomOf="@+id/twoRocketHatchCB" />

    <CheckBox
        android:id="@+id/cargoHatchCB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="23dp"
        android:layout_marginRight="23dp"
        android:scaleX="0.90"
        android:scaleY="0.90"
        android:text="Cargoship"
        app:layout_constraintBottom_toTopOf="@+id/textView17"
        app:layout_constraintEnd_toEndOf="@+id/threeRocketHatchCB" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="234dp"
        android:text="Successful"
        android:textSize="16sp"
        app:layout_constraintEnd_toStartOf="@+id/yesHatchCB"
        app:layout_constraintStart_toStartOf="@+id/yesHatchCB"
        app:layout_constraintTop_toTopOf="parent" />

    <CheckBox
        android:id="@+id/yesHatchCB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="23dp"
        android:layout_marginLeft="23dp"
        android:layout_marginTop="4dp"
        android:scaleX="0.90"
        android:scaleY="0.90"
        android:text="Yes"
        app:layout_constraintStart_toStartOf="@+id/cargoTeleCB"
        app:layout_constraintTop_toBottomOf="@+id/textView17" />

    <CheckBox
        android:id="@+id/noHatchCB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="23dp"
        android:layout_marginLeft="23dp"
        android:scaleX="0.90"
        android:scaleY="0.90"
        android:text="No"
        app:layout_constraintBaseline_toBaselineOf="@+id/yesHatchCB"
        app:layout_constraintStart_toEndOf="@+id/yesHatchCB" />

    <CheckBox
        android:id="@+id/cargoTeleCB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="35dp"
        android:layout_marginLeft="35dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="35dp"
        android:layout_marginRight="35dp"
        android:scaleX="0.90"
        android:scaleY="0.90"
        android:text="Cargo"
        app:layout_constraintEnd_toEndOf="@+id/cargoHatchCB"
        app:layout_constraintStart_toStartOf="@+id/hatchTeleCB"
        app:layout_constraintTop_toBottomOf="@+id/yesHatchCB" />

    <CheckBox
        android:id="@+id/rocketCargoTeleCB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginLeft="25dp"
        android:scaleX="0.90"
        android:scaleY="0.90"
        android:text="Rocketship"
        app:layout_constraintStart_toStartOf="@+id/cargoTeleCB"
        app:layout_constraintTop_toBottomOf="@+id/cargoTeleCB" />

    <CheckBox
        android:id="@+id/oneRocketCargoTeleCB"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_marginStart="122dp"
        android:layout_marginLeft="122dp"
        android:layout_marginTop="44dp"
        android:layout_marginBottom="45dp"
        android:scaleX="0.80"
        android:scaleY="0.80"
        android:text="Level 1"
        app:layout_constraintBottom_toBottomOf="@+id/threeRocketCargoCB"
        app:layout_constraintEnd_toEndOf="@+id/rocketCargoTeleCB"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/rocketCargoTeleCB" />

    <CheckBox
        android:id="@+id/twoRocketCargoCB"
        android:layout_width="wrap_content"
        android:layout_height="24dp"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="209dp"
        android:layout_marginRight="209dp"
        android:layout_marginBottom="21dp"
        android:scaleX="0.80"
        android:scaleY="0.80"
        android:text="Level 2 "
        app:layout_constraintBottom_toBottomOf="@+id/threeRocketCargoCB"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/oneRocketCargoTeleCB"
        app:layout_constraintTop_toTopOf="@+id/oneRocketCargoTeleCB" />

    <CheckBox
        android:id="@+id/threeRocketCargoCB"
        android:layout_width="wrap_content"
        android:layout_height="24dp"
        android:layout_marginTop="168dp"
        android:layout_marginBottom="168dp"
        android:scaleX="0.80"
        android:scaleY="0.80"
        android:text="Level 3"
        app:layout_constraintBottom_toBottomOf="@+id/button8"
        app:layout_constraintStart_toStartOf="@+id/twoRocketCargoCB"
        app:layout_constraintTop_toBottomOf="@+id/textView17" />

    <CheckBox
        android:id="@+id/cargoCargoTeleCB"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginEnd="41dp"
        android:layout_marginRight="41dp"
        android:scaleX="0.90"
        android:scaleY="0.90"
        android:text="Cargoship"
        app:layout_constraintBottom_toTopOf="@+id/textView18"
        app:layout_constraintEnd_toEndOf="@+id/threeRocketCargoCB"
        app:layout_constraintTop_toBottomOf="@+id/threeRocketCargoCB" />

    <Button
        android:id="@+id/button3"
        style="@android:style/Widget.Holo.Button"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="6dp"
        android:layout_marginLeft="6dp"
        android:layout_marginEnd="6dp"
        android:layout_marginRight="6dp"
        android:layout_marginBottom="13dp"
        android:onClick="startStopChronometer"
        android:text="START"
        app:layout_constraintBottom_toTopOf="@+id/button5"
        app:layout_constraintEnd_toEndOf="@+id/button5"
        app:layout_constraintStart_toStartOf="@+id/button5" />

    <Button
        android:id="@+id/button5"
        style="@android:style/Widget.Holo.Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="74dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="75dp"
        android:onClick="nextCycle"
        android:text="END CYCLE"
        app:layout_constraintBottom_toBottomOf="@+id/textView17"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/hatchTeleCB" />

    <Button
        android:id="@+id/button8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginLeft="40dp"
        android:layout_marginEnd="40dp"
        android:layout_marginRight="40dp"
        android:layout_marginBottom="60dp"
        android:onClick="startEndgameActivity"
        android:text="NEXT"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/button3"
        app:layout_constraintStart_toEndOf="@+id/noHatchCB" />

    <Chronometer
        android:id="@+id/chronometer1"
        android:layout_width="0dp"
        android:layout_height="32dp"

        android:layout_marginTop="16dp"
        android:layout_marginEnd="38dp"
        android:layout_marginRight="38dp"
        android:layout_marginBottom="10dp"
        android:countDown="false"
        android:textSize="22sp"
        app:layout_constraintBottom_toTopOf="@+id/button3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/cycleTitle"
        app:layout_constraintTop_toTopOf="parent" />

</android.support.constraint.ConstraintLayout>